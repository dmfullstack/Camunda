      <artifactId>camunda-engine</artifactId>
      <scope>provided</scope>
      <version>${camunda.version}</version>
    </dependency>
    
    <dependency>
      <groupId>org.jboss.spec.javax.servlet</groupId>
      <artifactId>jboss-servlet-api_3.0_spec</artifactId>
